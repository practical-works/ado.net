<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="Paramètres" type="MyADO.Données.Paramètres, MyADO"/>
  </configSections>
  <Paramètres>
    <Serveur Nom="localhost\AMBRATOLM_SQL">
      <BaseDonnées Nom="#MyADO">
        <Table Nom="Joueur">
          <Colonne Nom="id" Spécification="CléPrimaireAutoIncrémenté"/>
          <Colonne Nom="nom" Spécification="UniqueNonNulle"/>
          <Colonne Nom="pass" Spécification="NonNulle"/>
          <Colonne Nom="clé" Spécification="Unique"/>
        </Table>
        <Table Nom ="Partie">
          <Colonne Nom="num" Spécification="CléPrimaire"/>
          <Colonne Nom="mode" Spécification="Aucune"></Colonne>
          <Colonne Nom="dat" Spécification="Aucune"></Colonne>
          <Colonne Nom="deg" Spécification="AutoIncrémenté"></Colonne>
        </Table>
        <Table Nom ="Jouer">
          <Colonne Nom="id_joueur" Spécification="CléPrimaire"></Colonne>
          <Colonne Nom="num_partie" Spécification="CléPrimaire"></Colonne>
          <Colonne Nom="score" Spécification="Aucune"></Colonne>
        </Table>
      </BaseDonnées>
    </Serveur>
  </Paramètres>
</configuration>